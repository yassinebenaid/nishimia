var five = 5;
var ten = 10;

var add = fn(x, y) {
	return x + y;
};

var multiply = fn(x, y) {
	return x * y;
};

var devide = fn(x, y) {
	if(y > 0){
		return x / y;
	}else{
		return 0;
	}
};

var max = fn(x, y) {
	if(x > y){
		return x ;
	}

	if(x < y){
		return y ;
	}

	return x;
};

var addition = add(five, ten);
var multiplication = multiply(five, ten);
var devision = devide(five, ten);
var maximum = max(five, ten);